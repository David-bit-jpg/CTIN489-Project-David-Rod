        <uie:IntegerField label="X" value="20" name="mazeX_intField" tooltip="Minimum value is 3." style="font-size: 14px; margin-left: 5px; margin-right: 5px; margin-top: 1px; -unity-font-style: normal;" />
        <uie:IntegerField label="Y" value="20" name="mazeY_intField" tooltip="Minimum value is 3." style="font-size: 14px; max-width: none; margin-left: 5px; margin-right: 5px; margin-bottom: 1px; -unity-font-style: normal; margin-top: 1px;" />
