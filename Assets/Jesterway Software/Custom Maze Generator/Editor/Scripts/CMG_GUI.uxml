        <uie:ObjectField label="Floor material" name="floorMaterial_objectField" type="UnityEngine.Material, UnityEngine.CoreModule" tooltip="Assign a material for the floor of the maze." style="font-size: 14px; margin-left: 5px; margin-right: 5px; margin-bottom: 1px;" />
        <ui:Toggle label="Adjust UV orientation" name="rotateUVs_toggle" value="true" tooltip="Rotates all by-default upside-down faces to upright position, individually." style="font-size: 14px; margin-left: 5px; margin-right: 5px; margin-bottom: 1px; margin-top: 10px;" />
        <ui:Toggle label="Adjust UV scale" value="true" name="scaleUVs_toggle" tooltip="Allows for uniform texture tiling per face of any surface." style="font-size: 14px; margin-left: 5px; margin-right: 5px; margin-bottom: 1px;" />
